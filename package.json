{
  "name": "talks",
  "version": "3.1.0",
  "description": "Public talks provided either in English or French, about JavaScript or human-centric topics.",
  "private": true,
  "scripts": {
    "build": "hexo generate --bail",
    "build:pdf": "decktape reveal \"https://oncletom.io/talks/$YEAR/$EVENT/\" slides-thomas-parisot.pdf",
    "start": "hexo server",
    "new": "hexo new",
    "test": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/oncletom/talks.git"
  },
  "keywords": [
    "talks",
    "javascript",
    "french",
    "english",
    "conference",
    "sharing",
    "knowledge"
  ],
  "author": "Thomas Parisot (https://oncletom.io)",
  "license": "BSD",
  "readmeFilename": "README.md",
  "hexo": {
    "version": "3.9.0"
  },
  "dependencies": {
    "hexo": "^3.9.0",
    "hexo-algolia": "^1.3.1",
    "hexo-cli": "^2.0.0",
    "hexo-generator-feed": "^1.2.2",
    "hexo-generator-index": "^0.2.1",
    "hexo-renderer-ejs": "^0.3.1",
    "hexo-renderer-markdown-it": "^3.4.1",
    "hexo-server": "^0.3.3",
    "hexo-theme-oncletom": "github:oncletom/hexo-theme#v2.8.2",
    "markdown-it-attrs": "^3.0.1"
  },
  "devDependencies": {
    "decktape": "^2.9.3"
  }
